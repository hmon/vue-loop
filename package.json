{
  "name": "vue-loop",
  "version": "0.0.1",
  "description": "Vue Loop Plugin",
  "main": "dist/vue-loop.js",
  "types": "src/types/index.d.ts",
  "files": [
    "dist",
    "src",
    "nuxt"
  ],
  "scripts": {
    "build": "cross-env NODE_ENV=production webpack --config webpack.config.js"
